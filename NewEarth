import java.time.*;

public class Base {
    int Air = 1;
    double Grass = 1/0.0;

    static boolean lightTime = false;
}
class Sun extends Base {

            public static boolean light () {
                LocalTime LightTimeStart = LocalTime.of(8, 0, 0);
                LocalTime LightTimeEnd = LocalTime.of(20, 0, 0);
                LocalTime CurrentTime = LocalTime.now();
                int CompStart = CurrentTime.compareTo(LightTimeStart);
                int CompEnd = CurrentTime.compareTo(LightTimeEnd);
                if (CompStart > 0 && CompEnd < 0) {
                    lightTime = true;
                }
                return lightTime;
            }
    }
class Tree extends Base {
    int i;

    private int ProOxyg() {
        {
            if (Sun.light()) {
                for (i = 0; i <= 12; i++) {
                    Air++;
                }
            }
            else {
                System.out.println("Now is dark time");
            }
            }
            return Air;
        }
}
class Frog extends Base {
    int stomach = 0;
    int i = 0;

    private void Eat() {
        if (Sun.light() && stomach == 0) {
            for (i = 12; i >= 0; i--) {
                Grass--;
                stomach++;
            }
        } else {
            System.out.println("Frog is not hungry");
            for (i=0; i <= 12; i++) {
                stomach--;
            }
            Eat();
        }
    }

    private void Breath() {
        if (Sun.light()) {
            for (i = 12; i >= 0; i--) {
                Air--;
            }
        } else {
            System.out.println("Frog is sleeping now");
        }
    }
}

class Main {
    public static void main(String[] args) {
        boolean fff = Sun.light();
        Sun sun = new Sun();
        Tree tree = new Tree();
        Frog frog = new Frog();
            if (Sun.light()) {
                System.out.println("Now we are living");
        }
    }
}
